{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "checkJs": false,
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": [
      "dom",
      "ESNext"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmit": false,
    "noImplicitAny": false,
    "paths": {
      "core": ["packages/core/src"],
      "plugin-appwrite": ["packages/plugin-appwrite/src"],
      "plugin-blog": ["packages/plugin-blog/src"]
    },
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "ES2019",
    "types": [
      "node"
    ],
    "useUnknownInCatchVariables": false
  },
  "exclude": [
    "node_modules",
    "build",
    "**/*/dist",
    "**/*/out",
    "tests",
    "**/*.test.ts",
    "**/webpack.*"
  ]
}