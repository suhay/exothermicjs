{
  "name": "@exothermic/plugin-markdown",
  "version": "1.4.0",
  "description": "Markdown plugin for ExothermicJS",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suhay/exothermicjs.git"
  },
  "author": "Matt Suhay <matthew.suhay@gmail.com>",
  "homepage": "https://github.com/suhay/exothermicjs#readme",
  "license": "MIT",
  "module": "src/index.js",
  "main": "src/index.js",
  "browser": "dist/exothermicjs-plugin-markdown.js",
  "files": [
    "dist/",
    "src/"
  ],
  "scripts": {
    "watch": "webpack --env.NODE_ENV=development --watch --mode development",
    "build": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "build:all": "yarn build && yarn build:prod",
    "profile": "webpack --profile --mode development --json > stats.json",
    "profile:prod": "webpack --profile --mode production --json > stats.json"
  },
  "bugs": {
    "url": "https://github.com/suhay/exothermicjs/issues"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "localforage": "^1.7.3",
    "react-markdown": "^4.0.4",
    "react-simplemde-editor": "^3.6.22",
    "shortid": "^2.2.14"
  },
  "peerDependencies": {
    "@exothermic/core": "^1.4.0",
    "react": "^16.8.6",
    "reactn": "^2.1.7"
  }
}
