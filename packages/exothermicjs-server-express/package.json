{
  "name": "exothermicjs-server-express",
  "version": "0.1.0",
  "description": "Express Server for ExothermicJS",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suhay/exothermicjs.git"
  },
  "author": "Matt Suhay <matthew.suhay@gmail.com>",
  "homepage": "https://github.com/suhay/exothermicjs#readme",
  "license": "MIT",
  "module": "src/index.js",
  "main": "dist/exothermicjs-server-express.min.js",
  "files": [
    "dist/",
    "src/"
  ],
  "bin": {
    "exothermic-server": "./bin/exothermic-server"
  },
  "scripts": {
    "start": "node ./bin/exothermic-server",
    "watch": "webpack --env.NODE_ENV=production --watch --mode production",
    "build": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "build:all": "yarn build && yarn build:prod",
    "dev": "webpack --env.NODE_ENV=development --watch --mode development"
  },
  "bugs": {
    "url": "https://github.com/suhay/exothermicjs/issues"
  },
  "dependencies": {
    "connect-ensure-login": "^0.1.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-fileupload": "^1.1.2-alpha.1",
    "helmet": "^3.16.0",
    "http-errors": "^1.7.2",
    "morgan": "^1.9.1"
  },
  "peerDependencies": {
    "exothermicjs": "^1.3.1"
  }
}
